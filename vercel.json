{
    "builds": [
      {
        "src": "nuxt.config.js",
        "use": "@nuxtjs/vercel-builder",
        "config": {
          "serverFiles": ["server/**"]
        }
      }
    ],
    "headers": [
      {
        "source": "/img/(.*)",
        "headers": [
          { "key": "Cache-Control", "value": "public, max-age=36000, immutable" }
        ]
      }
    ]
}