{
    "name": "flone",
    "version": "1.0.0",
    "author": "Uzzal",
    "private": true,
    "scripts": {
        "dev": "concurrently \"nuxt dev --config-file server/nuxt.config.js\" \"nuxt dev --config-file client/nuxt.config.js\"",
        "build": "concurrently \"nuxt build --config-file server/nuxt.config.js && node server/index.js\" \"nuxt generate --config-file client/nuxt.config.js\"",
        "start": "concurrently \"node server/index.js\" \"nuxt start --config-file client/nuxt.config.js\""
    },
    "dependencies": {
        "@nuxtjs/axios": "^5.13.6",
        "@nuxtjs/style-resources": "^1.2.1",
        "@popperjs/core": "^2.11.4",
        "bcrypt": "^5.1.1",
        "bootstrap": "^5.1.3",
        "core-js": "^3.37.1",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "jsonwebtoken": "^9.0.2",
        "mysql2": "^3.10.1",
        "node-cron": "^3.0.3",
        "nuxt": "^2.15.8",
        "sequelize": "^6.37.3",
        "sequelize-cli": "^6.6.2",
        "swiper": "^5.3.7",
        "vue-awesome-swiper": "^4.1.1",
        "vue-count-to": "^1.0.13",
        "vue-js-modal": "^2.0.1",
        "vue-notification": "^1.3.20",
        "vue-observe-visibility": "^1.0.0",
        "vue-pagination-2": "^3.0.91",
        "vuex-persistedstate": "^2.7.1"
    },
    "devDependencies": {
        "concurrently": "^8.2.2",
        "sass": "^1.49.9",
        "sass-loader": "^10.1.1",
        "webpack": "^4.46.0"
    }
}
